{
  "easycom": {
    "^cl-(.*)": "@/components/cl-$1/index.vue",
    "^db-(.*)": "@/components/db-$1/index.vue",
    "^fui-(.*)": "@/uni_modules/firstui/fui-$1/fui-$1.vue"
  },
  "pages": [
    {
      "path": "pages/home/index",
      "name": "home",
      "aliasPath": "/",
      "meta": {
        "title": "首页"
      },
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mine/index",
      "name": "mine",
      "meta": {
        "title": "我的"
      },
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/index",
      "name": "login",
      "meta": {
        "title": "登录"
      },
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "backgroundTextStyle": "dark",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "宜昌市机关事务服务中心数据融合平台",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F1F4FA",
    "backgroundColorTop": "#F1F4FA",
    "backgroundColorBottom": "#F1F4FA"
  },
  "tabBar": {
    "color": "#333333",
    "selectedColor": "#1087DD",
    "backgroundColor": "#FFFFFF",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "首页",
        "iconPath": "static/imgs/tabbar/home.png",
        "selectedIconPath": "static/imgs/tabbar/home_active.png"
      },
      {
        "pagePath": "pages/mine/index",
        "text": "我的",
        "iconPath": "static/imgs/tabbar/server.png",
        "selectedIconPath": "static/imgs/tabbar/server_active.png"
      }
    ]
  },
  "resizable": true
}
